@model IEnumerable<WebApplication4.Models.Lesson>
@{
   
}
<html>
<head>

    <style>

        body {
            --main-font: 'Roboto', sans-serif;
            --header-font: 'Anton', sans-serif;
            --main-color: #FA9307;
            --light-color: #FA9307;
            --white: #fff;
            --black: #222;
            --gray: #888;
            --dark-white: #f5f5f5;
            --shadow: 0px 0px 6px 2px var(--dark-white);
            --space: 15px;
            --screw: 4px;
            --link-color: #FA9307;
            background-color: var(--white) !important;
            color: var(--black) !important;
        }

        h1, h2, h3, h4, h5, h6, .logo {
            font-family: var(--header-font);
            text-transform: uppercase;
            margin-top: 0;
        }

        .logo {
            font-size: 1.6em;
        }

        a {
            color: var(--light-color);
        }

        td {
            color: var(--black);
        }

        .shadow {
            box-shadow: var(--shadow);
        }

        form.controlled > * {
            margin-bottom: var(--space);
        }

        footer {
            background-color: var(--black);
            color: var(--white);
            height: 40px;
            display: flex !important;
            justify-content: center;
            align-items: flex-end;
            box-shadow: var(--shadow);
        }

        .spaced-bottom {
            margin-bottom: var(--space);
        }

        button,
        input[type='submit'],
        input[type='button'] {
            background-color: var(--light-color) !important;
            border-color: var(--main-color) !important;
            color: var(--white) !important;
            border-radius: var(--screw);
        }

        .navbar-brand {
            color: var(--white) !important;
            transition: all .3s;
        }

            .navbar-brand:hover {
                color: var(--link-color) !important;
            }

        .navbar-inverse {
            max-height: 40px;
            box-shadow: var(--shadow);
        }

        .profile .dropdown-toggle {
            color: var(--white) !important;
            max-height: 40px;
            border: 0;
        }

        .dropdown-menu {
            box-shadow: none;
        }

        .profile .dropdown-toggle:hover {
            background-color: var(--black) !important;
        }

        .body-content {
            min-height: calc(100vh - 110px);
        }
    </style>
</head>
<body>
    <br />
    <div class="container">
        <div class="row">
            <table class="table">

                @foreach (var item in Model)
                {

                <tr>

                    <td>@item.TITLE_LESSON </td>
                   
                    <td>
                       
                        <a href="~/Admin/ChangeLesson/?id=@item.ID_LESSON">Изменить</a>
                    </td>
                    
                </tr>
                }
            </table>
        </div>
    </div>
</body>
